{
  "include": ["src"],
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "sourceMap": false,
    "strict": true,
    "noImplicitAny": true,
    "module": "es2020",
    "target": "es2020"
  }
}
